{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "baml",
      "camelcase": "Baml",
      "title": "BAML",
      "scope": "source.baml",
      "file-types": [
        "baml"
      ],
      "injection-regex": "^baml$",
      "class-name": "TreeSitterBaml"
    }
  ],
  "metadata": {
    "version": "0.81.0",
    "license": "Apache License 2.0",
    "description": "BAML is a DSL for AI applications.",
    "authors": [
      {
        "name": "BoundaryML"
      }
    ],
    "links": {
      "repository": "https://github.com/boundaryml/baml"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}